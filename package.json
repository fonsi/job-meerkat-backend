{
    "scripts": {
        "lint": "eslint --fix",
        "prettier": "prettier --list-different './**/*.{js,ts}'",
        "typecheck": "tsc --noEmit",
        "local": "npm run lint && sls invoke local --function",
        "scrap-company": "npm run local devScrapCompany -- --path src/company/infrastructure/dev/scrapCompany.event.json",
        "deploy:dev": "npm run lint && serverless deploy -v",
        "deploy:prod": "npm run lint && serverless deploy -v --stage=production",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.16.2",
        "eslint": "^9.9.1",
        "globals": "^15.9.0",
        "jest": "^29.7.0",
        "serverless": "^4.4.18",
        "ts-jest": "^29.2.5",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.3.0"
    },
    "volta": {
        "node": "20.17.0"
    },
    "dependencies": {
        "@aws-sdk/client-api-gateway": "^3.686.0",
        "@aws-sdk/client-dynamodb": "^3.686.0",
        "@aws-sdk/client-lambda": "^3.686.0",
        "@aws-sdk/client-sqs": "^3.686.0",
        "cheerio": "^1.0.0",
        "openai": "^4.57.0"
    }
}